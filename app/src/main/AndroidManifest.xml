<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.longyun.steelx">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SteelLord"
        tools:targetApi="31">

        <activity
            android:name="com.longyun.steelx.TestActivity"
            android:exported="false" />
        <activity
            android:name="com.longyun.steelx.WipeUpActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.longyun.steelx.TwistActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.longyun.steelx.ShakeActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.longyun.steelx.SensorActivity"
            android:exported="false" />
        <activity
            android:name="com.longyun.steelx.LottieActivity"
            android:exported="false" />
        <activity
            android:name="com.longyun.steelx.BlurActivity"
            android:exported="false" />
        <activity
            android:name="com.longyun.steelx.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>